<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroiund"
    tools:context=".Activities.frontPageActivity">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/biceps_layout"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/arms">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="serif"
                    android:text="BICEPS"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:fontFamily="serif"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/shoulder"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/shoulders">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:fontFamily="serif"
                    android:text="SHOULDERS"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:fontFamily="serif"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Abs"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/abs">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="36dp"
                    android:fontFamily="serif"
                    android:text="ABS"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    app:fontFamily="serif"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Chest"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/chest">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:fontFamily="serif"
                    android:text="CHEST"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Forearms"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/forearm">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="serif"
                    android:text="FOREARMS"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Triceps"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/triceps">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="serif"
                    android:text="TRICEPS"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Back"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/back">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="serif"
                    android:text="BACK"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Legs"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/legs">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginBottom="8dp"
                    android:fontFamily="serif"
                    android:text="LEGS"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />
            </android.support.constraint.ConstraintLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="5dp" />

            <android.support.constraint.ConstraintLayout
                android:id="@+id/Calves"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:background="@drawable/calves">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginBottom="4dp"
                    android:fontFamily="serif"
                    android:text="CALVES"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="#4CAF50"
                    android:textSize="36sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</android.support.constraint.ConstraintLayout>